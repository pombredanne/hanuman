{
  "manifest_version": 2,

  "name": "Hanuman",
  "description": "This extension does mysterious things with lobbying",
  "version": "0.1",

  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },

  "browser_action": {
    "default_icon": {
      "19": "images/icon19.png",
      "38": "images/icon38.png"
    },
    "default_popup": "popup.html",
    "default_title": "Hanuman"
  },
  "permissions": [
    "activeTab",
    "https://*/*",
    "http://*/*",
    "webNavigation"
  ]
}
