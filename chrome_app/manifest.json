{
  "manifest_version": 2,

  "name": "Hanuman",
  "description": "This extension does mysterious things with lobbying",
  "version": "0.1",

  "icons": {
    "16": "images/icon16.png",
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  },

  "app": {
    "background": {
      "scripts": ["js/background.js"]
    }
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "webview",
    "storage"
  ]
}
